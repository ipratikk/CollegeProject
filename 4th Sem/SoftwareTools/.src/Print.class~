  adhar.Caption = Sale.TextBox2.Text
  add.Caption = Sale.TextBox3.Text
  ph.Caption = Sale.TextBox4.Text
  Label1.Font.Bold = True
  Label2.Font.Bold = True
  Label3.Font.Bold = True
  Label4.Font.Bold = True
  

End
